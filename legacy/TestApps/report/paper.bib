/@inproceedings{fidge_vector_clocks_1988,
    abstract = {Timestamping is a common method of totally ordering events in concurrent programs. However, for applications requiring access to the global state, a total ordering is inappropriate. This paper presents algorithms for timestamping events in both synchronous and asynchronous message-passing programs that allow for access to the partial ordering inherent in a parallel system. The algorithms do not hcnage the communications graph or require a central timestamp issuing authority.},
    address = {University of Queensland, Australia},
    author = {Fidge, Colin J.},
    booktitle = {11th Australian Computer Science Conference},
    citeulike-article-id = {8196086},
    keywords = {dist-sys, vector-clocks},
    pages = {55--66},
    posted-at = {2011-03-25 00:09:30},
    priority = {2},
    title = {Timestamps in {M}essage-{P}assing {S}ystems that {P}reserve the {P}artial {O}rdering},
    year = {1988}
}

@inproceedings{mattern_vector_clocks_1989,
    abstract = {A distributed system can be characterized by the fact that the global state is distributed and that a common time base does not exist. However, the notion of time is an important concept in every day life of our decentralized "real world" and helps to solve problems like getting a consistent population census or determining the potential causality between events. We argue that a linearly ordered structure of time is not (always) adequate for distributed systems and propose a generalized non-standard model of time which consists of vectors of clocks. These clock-vectors are partially ordered and form a lattice. By using timestamps and a simple clock update mechanism the structure of causality is represented in an isomorphic way. The new model of time has a close analogy to Minkowski's relativistic spacetime and leads among others to an interesting characterization of the global state problem. Finally, we present a new algorithm to compute a consistent global snapshot of a distributed sy...},
    author = {Mattern, Friedemann},
    booktitle = {Parallel and Distributed Algorithms},
    citeulike-article-id = {5727038},
    citeulike-linkout-0 = {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.47.7435},
    citeulike-linkout-1 = {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.47.7435},
    keywords = {dist-sys, vector-clocks},
    pages = {215--226},
    posted-at = {2011-03-25 00:04:26},
    priority = {2},
    title = {Virtual {T}ime and {G}lobal {S}tates of {D}istributed {S}ystems},
    url = {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.47.7435},
    year = {1989}
}

